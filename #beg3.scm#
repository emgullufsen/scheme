					; Eric Gullufsen
; Ackerman's function
(define (A x y)
  (cond ((= y 0) 0)
	((= x 0) (* 2 y))
	((= y 1) 2)
	(else (A (- x 1)
		 (A x (- y 1))))))

					; A function f is defined by the rule that...

					; f(n) = n, if n < 3
					; f(n) = f(n-1) + 2f(n-2) + 3f(n-3), if n >= 3
					; recursive
(define (f n)
  (if (< n 3)
      n
      (+ (f (- n 1)) (* 2 (f (- n 2))) ())))
	